{
  "name": "Text Editing Tests",
  "tests": [
    {
      "name": "Delete character",
      "input": "hello world",
      "commands": "x",
      "cursor": [0, 0]
    },
    {
      "name": "Delete word",
      "input": "hello world",
      "commands": "dw",
      "cursor": [0, 0]
    },
    {
      "name": "Join lines",
      "input": "hello\nworld",
      "commands": "J",
      "cursor": [0, 0]
    },
    {
      "name": "Replace character",
      "input": "hello",
      "commands": "ra",
      "cursor": [0, 0]
    },
    {
      "name": "Delete line",
      "input": "line1\nline2\nline3",
      "commands": "dd",
      "cursor": [1, 0]
    }
  ]
}
set nocompatible
set scrolloff=3
set showcmd
set number
set ruler
set visualbell t_vb=
set novisualbell
set hlsearch
set incsearch
set showmatch
set ignorecase
set smartcase
set ai
set tabstop=2
set shiftwidth=2
set softtabstop=2
set backspace=indent,eol,start
set nobackup
syntax on
filetype on
filetype indent on
filetype plugin indent on